{
    "name": "jmondi/phpunit-tdd-boilerplate",
    "description": "Boilerplate for a TDD project.",
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.6",
        "phploc/phploc": "~3.0"
    },
    "require": {
        "composer/installers": "~1.0",
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Jmondi\\Guts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmondi\\Guts\\Tests\\": "src"
        }
    }
}
